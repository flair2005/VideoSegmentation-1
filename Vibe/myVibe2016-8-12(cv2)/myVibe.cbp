<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="myVibe" />
		<Option makefile_is_custom="1" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Debug">
				<Option output="./myMain" prefix_auto="1" extension_auto="1" />
				<Option working_dir="./" />
				<Option object_output="./" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Option use_console_runner="0" />
				<Compiler>
					<Add option="-g" />
				</Compiler>
				<MakeCommands>
					<Build command="$make -f $makefile " />
					<CompileFile command="$make -f $makefile $file" />
					<Clean command="$make -f $makefile clean" />
					<DistClean command="$make -f $makefile distclean$target" />
					<AskRebuildNeeded command="$make -q -f $makefile" />
					<SilentBuild command="$make -f $makefile  &gt; $(CMD_NULL)" />
				</MakeCommands>
			</Target>
		</Build>
		<Compiler>
			<Add option="-Wall" />
		</Compiler>
		<Unit filename="./myMain.cpp">
			<Option target="&lt;{~None~}&gt;" />
		</Unit>
		<Unit filename="Makefile" />
		<Unit filename="main-opencv.cpp" />
		<Unit filename="myVibe.cpp">
			<Option target="&lt;{~None~}&gt;" />
		</Unit>
		<Unit filename="myVibe.h">
			<Option target="&lt;{~None~}&gt;" />
		</Unit>
		<Unit filename="vibe-background-sequential.c">
			<Option compilerVar="CC" />
		</Unit>
		<Unit filename="vibe-background-sequential.h" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
			<lib_finder disable_auto="1" />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
